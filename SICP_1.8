(define (square x) (* x x))
(define (cube x) (* x x x))
(define (cub x)
    (define (good-enough? guess)
        (< (abs (- (cube guess) x)) 0.001))
    (define (improve guess)
        (/ (+ (/ x (square guess)) (* 2 guess)) 3))
    (define (cube-iter guess)
        (if (good-enough? guess)
            guess
            (cube-iter (improve guess))))
    (cube-iter 1.0))